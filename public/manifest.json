{
  "name": "UMP Toolbox",
  "version": "0.0.3.2",
  "description": "This extension is an unofficial Mixpanel tool pane to help with your Mixpanel implementation.",
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "manifest_version": 2,
  "devtools_page": "dev-tools.html",
  "permissions": ["webRequest", "activeTab", "<all_urls>"],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "page_action": {},
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk=' 'sha256-QHj/uDT9JsKgW4j/jWyBvs5msxhEfQeAGPjhqbI6UhI='; object-src 'self'",
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["http://localhost:3000/*", "http://localhost/*"]
  }
}
